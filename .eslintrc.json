{
    "env": {
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended",
        "plugin:security/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "plugins": [
        "jest",
        "security",
        "prettier"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "consistent-return": "off",
        "max-len": "off",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "jest/expect-expect": "off",
        "security/detect-object-injection": "off"
    }
}
